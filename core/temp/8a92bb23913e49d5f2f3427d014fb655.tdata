    ty = window.ty || {};
    ty.home = ty.home || {};
    ty.home.data = ty.home.data || {};
    ty.home.data.minDate = 'Fri, 07-Mar-2025';
    ty.home.data.defaultDate = 'Fri, 07-Mar-2025';
