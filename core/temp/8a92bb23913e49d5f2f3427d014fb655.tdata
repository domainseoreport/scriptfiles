//for bus routes count update
function busroutesCnt(val) {
    var busroutes = "";

    setTimeout(function() {
        if (val < busroutes) {
            $("#busroutes").text(val + "+");
            busroutesCnt(val + 50);
        } else {
            $("#busroutes").text(busroutes + "+");
        }
    }, 1);
}

//for bus counts update
function busesCnt(val) {
    var buses = "";

    setTimeout(function() {
        if (val < buses) {
            $("#buses").text(val + "+");
            busesCnt(val + 1);
        } else {
            $("#buses").text(buses + "+");
        }
    }, 1);
}

//for daily users count update
function dailyusersCnt(val) {
    try {
        var dailyusers = "";
        setTimeout(function() {
            if (val < dailyusers) {
                $("#dailyusers").text(val + "+");
                dailyusersCnt(val + 100);
            } else {
                $("#dailyusers").text(dailyusers + "+");
            }
        }, 1);
    } catch {

    }
}

var modal = null;

function viewMore(srno) {

    modal = document.getElementById("notificationModal");
    modal.style.display = "block";

    $("#notificationModal #notification-header").text($("#notificationWrap-" + srno).attr(
        "data-header"));
    $("#notificationModal #notification-text").text($("#notificationWrap-" + srno).attr("data-text"));

    $("#notificationModal span").click(function() {
        modal.style.display = "none";
    });
}

