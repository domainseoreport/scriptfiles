let topRouteFlag = 0;
let onScrollLoadImg = 0;
let loyaltyflag = 0;
$(window).scroll(function() {
    //load top route images 
    if (topRouteFlag == 0) {
        topRouteFlag = 1;
        $("#topRoutesList li").each(function() {
            var imgSrc = $(this).attr("data-src");
            var imgAlt = $(this).attr("data-alt");
            $(this).find(".top-route-image").append(
                "<img class=\"popDestImg\" src=\"/img/theme68/top-routes/" + imgSrc + "\" alt=\"" +
                imgAlt + "\" height=\"240\">");
        });
    }

    if (onScrollLoadImg == 0) {
        onScrollLoadImg = 1;
        /* pull slider 3 images */
        $.ajax({
            type: 'GET',
            url: 'api/others/slider3Images',
            success: function(result) {
                if(result['images']){
                    var responses = result['images'];
                    var len = responses.length;
                    if (len > 0) {
                        $.each(responses, function(index) {
                            var imgHtml = "<li><div class=\"image-box\">";
                            imgHtml += "<img class=\"responsive\" src=\"" +
                                responses[index].image +
                                "\" alt=\"" + responses[
                                    index].alttag + "\"  width=\"505\" height=\"354\" >";
                            imgHtml += "</div></li>";
                            $(imgHtml).appendTo('#image-gallery');
                        });
                        triggerslider3();
                    }
                }
            }
        });

        /* pull slider 2 images */
        $.ajax({
            type: 'GET',
            url: 'api/others/slider2Images',
            success: function(result) {
                if(result['images']){
                    var responses = result['images'];
                    var len = responses.length;
                    if (len > 0) {
                        $.each(responses, function(index) {
                            var imgHtml = "<li><div class=\"image-box\">";
                            imgHtml += "<img class=\"responsive\" src=\"" +
                                responses[index].image +
                                "\" alt=\"" + responses[
                                    index].alttag + "\" width=\"607\" height=\"360\">";
                            imgHtml += "</div></li>";
                            $(imgHtml).appendTo('#photo-gallery');
                        });
                        triggerslider2();
                    }
                }
            }
        });
    }

    //for layalty slider set on scroll
    var loyalty = document.getElementById("slider_loyalty");

    if (loyalty != null) {

        var loyatyposition = loyalty.getBoundingClientRect();

        loyatyposition = loyatyposition.top;
        if (loyatyposition < 500 && loyaltyflag == 0) {

            var width = window.innerWidth;
            var sliderItems = 4;
            if (width >= 768 && width <= 992) {
                sliderItems = 2;
            } else if (width >= 992 && width <= 1200) {
                sliderItems = 3;
            }

            loyaltyflag = 1;
            var slider_loyalty = $('#slider_loyalty').lightSlider({
                gallery: false,
                item: sliderItems,
                thumbItem: 0,
                slideMargin: 25,
                speed: 500,
                auto: false,
                loop: true,
                pager: false,
                slideMove: 1,
                addClass: '',
                controls: false,
                onSliderLoad: function() {
                    $('#slider_loyalty').removeClass('cS-hidden');
                },
                onBeforeSlide: function() {

                }
            });

            $('#loyaltySPrev').click(function() {
                slider_loyalty.goToPrevSlide();
            });

            $('#loyaltySNext').click(function() {
                slider_loyalty.goToNextSlide();
            });
        }
    }

});

