    var topRoutesSlider = $('#topRoutesList').lightSlider({
        item: 3,
        speed: 500,
        loop: true,
        slideMargin: 0,
        slideMove: 1,
        pager: true,
        controls: false,
        auto: false,
        onSliderLoad: function() {
            $('#topRoutesList').removeClass('cS-hidden');
            $('.topRoutesNext').click();
        },
        onBeforeSlide: function(el) {
            $("#topRoutesList li").removeClass("slideLeft").removeClass("slideCenter").removeClass(
                "slideRight");
        },
        onAfterSlide: function(el) {
            setTimeout(() => {
                $("#topRoutesList .lslide.active").addClass("slideLeft");
                $("#topRoutesList .lslide.active").next("li").addClass("slideCenter");
                $("#topRoutesList .lslide.active").next("li").next("li").addClass(
                    "slideRight");
            }, 100);
        }
    });

    $('.topRoutesNext').click(function() {
        topRoutesSlider.goToNextSlide();
    });

    $('.topRoutesPrev').click(function() {
        topRoutesSlider.goToPrevSlide();
    });

    setTimeout(function() {
        busroutesCnt(0);
        busesCnt(0);
